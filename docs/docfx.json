{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
            "exclude": [ "**/unittests/**" ],
          "src": "../src"
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "**/*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "*.md", "toc.yml" ]
      }
    ],
    "overwrite": "specs/*.md",
    "globalMetadata": {
      "_appTitle": "CSXShell",
      "_enableSearch": true
    },
    "markdownEngineName": "markdig",
    "dest": "_site",
    "template": ["default", "templates/DiscordFX/discordfx"]
  }
}
